---
interface Props {
  class?: string;
  path: string;
}

const { class: className, path, ...attrs } = Astro.props;
---

<svg
  class={className}
  fill="none"
  viewBox="0 0 24 24"
  stroke-width="1.5"
  stroke="currentColor"
  aria-hidden="true"
  {...attrs}
>
  <path stroke-linecap="round" stroke-linejoin="round" d={path}></path>
</svg>
